    header(class="HeaderMain")
        div(class="HeaderMain__LogoMain")
            figure(class="HeaderMain__LogoMain__box")
                img(src="./images/logo-blanco@2x.png" id="logo_white")

        div(class="HeaderMain__NavMain")
            nav(class="HeaderMain__NavMain__Container")
                div(class="HeaderMain__NavMain__Container--item")
                    figure
                        a(href="#")
                            img(src="./images/redes-sociales/facebook_white.png")
                div(class="HeaderMain__NavMain__Container--item")
