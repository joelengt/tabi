                article(class="FooterMain__right--text")
                    p Tabi Assist SAC - Todos los derechos reservados 2017

            div(class="FooterMain__left")
                article(class="FooterMain__left--icons")
                    figure
                        img(src="./images/footerMain/culqi_logo.png")
                    figure(style="padding-left: .7rem;")
                        img(src="./images/FooterMain/visa_2.png")

                article(class="FooterMain__left--email")
                    figure
                        img(src="./images/footerMain/email.png")
                    div(class="text")
                        p(class="email") hola@assistabi.com

